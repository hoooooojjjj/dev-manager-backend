# Anthropic API Key
ANTHROPIC_API_KEY=anthropic_api_key

# Database Configuration
# Note: When using Docker, DB_USERNAME is automatically set to 'root' in containers
# For local development without Docker, use DB_HOST=127.0.0.1
DB_HOST=127.0.0.1
DB_PORT=3306
DB_USERNAME=username
DB_PASSWORD=password
DB_DATABASE=dev_manager

# GitHub OAuth - Development
GITHUB_CLIENT_ID_DEV=your_github_dev_client_id
GITHUB_CLIENT_SECRET_DEV=your_github_dev_client_secret

# GitHub OAuth - Production
GITHUB_CLIENT_ID_PROD=your_github_prod_client_id
GITHUB_CLIENT_SECRET_PROD=your_github_prod_client_secret

# JWT Configuration
JWT_SECRET=CHANGE_THIS_TO_A_STRONG_RANDOM_SECRET_IN_PRODUCTION
JWT_ACCESS_TOKEN_EXPIRES_IN=900
JWT_REFRESH_TOKEN_EXPIRES_IN=604800
